{
  "name": "campaign-buddy",
  "version": "1.0.0",
  "description": "A web based D&D campaign management tool",
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@blueprintjs/core": "^3.12.0",
    "@blueprintjs/labs": "^0.16.3",
    "@blueprintjs/select": "^3.6.1",
    "babel": "^6.23.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cryptr": "^4.0.2",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "happycalculator": "^1.3.6",
    "http-status-codes": "^1.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "keyboardjs": "^2.5.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-mock-strategy": "^1.1.1",
    "prop-types": "^15.7.1",
    "react": "^16.7.0",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "^16.7.0",
    "react-inlinesvg": "^0.8.4",
    "react-panelgroup": "^1.0.7",
    "react-quill": "^1.3.3",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.8.6",
    "sanitize-html": "^1.20.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "webpack": "^4.29.0"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-plugin-react": "^7.12.4",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "webpack --mode=development",
    "server": "webpack --mode=development --config=webpack.server.js && node server.bundle.js",
    "lint": "eslint .",
    "lint:autofix": "eslint . --fix",
    "build:database": "node database/bundle-sql.js"
  },
  "jest": {
    "projects": [
      {
        "displayName": "client",
        "setupFilesAfterEnv": [
          "<rootDir>/testing/setup-tests-client.js"
        ],
        "roots": [
          "<rootDir>/front-end"
        ],
        "moduleNameMapper": {
          "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testing/__mocks__/fileMock.js",
          "\\.(css|less)$": "identity-obj-proxy"
        }
      },
      {
        "displayName": "server",
        "setupFilesAfterEnv": [
          "<rootDir>/testing/setup-tests-server.js"
        ],
        "roots": [
          "<rootDir>/back-end"
        ],
        "testEnvironment": "node",
        "moduleNameMapper": {
          "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testing/__mocks__/fileMock.js"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "\u0016https://github.com/cross21/dnd-webMaster"
  },
  "author": "Joseph Stewart",
  "license": "ISC"
}
